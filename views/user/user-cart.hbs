                                                </div><!-- End .cart-product-quantity --> --}}
                                            
                                            </td>
                                      
											<td id="hello{{this.product._id}}">₹ {{multiply this.product.NewPrice this.quantity}} /-</td>
                                        
											<td class="remove-col"><button class="btn-remove" onclick="remove('{{this._id}}','{{this.product._id}}')"><i class="icon-close"></i></button></td>
                                          
										</tr>
                                         {{/each}}
