<script>
    $('#addresss').submit((e)=>{
    e.preventDefault();
    $.ajax({
        url: '/addaddress',
        method: 'post',
        data:$('#addresss').serialize(),
        
        success:(response)=>{
            if(response){
                document.getElementById("addd").value=""
                document.getElementById("addd1").value=""
                document.getElementById("addd2").value=""
                 document.getElementById("addd3").value=""
                  document.getElementById("addd4").value=""
                   document.getElementById("addd5").value=""
                    document.getElementById("addd6").value=""
                     document.getElementById("addd7").value=""
                      document.getElementById("addd8").value=""
                document.getElementById("addressdetails").innerHTML="Address Details Updated Successfully!"
            }
        }
    })
})

$('#passwordform').submit(function(e){
    e.preventDefault();
    $.ajax({
        url:'/updatepassword',
        data:$('#passwordform').serialize(),
        method:'patch',
        success:(response)=>{
            if(response.status){
                document.getElementById("pass").value=""
                document.getElementById("pass1").value=""
                document.getElementById("pass2").value=""
                document.getElementById("success").innerHTML="Password Updated Successfully!"
            }else{
                document.getElementById("pass").value=""
                document.getElementById("pass1").value=""
                document.getElementById("pass2").value=""
                document.getElementById("danger").innerHTML="Password Doesn't Match!"
            }

        }
    })
})
function modalclose(){
    document.getElementById("success").innerHTML=""
    document.getElementById("danger").innerHTML=""
    document.getElementById("addressdetails").innerHTML=""
    location.reload()

}
function change(){
    document.getElementById("savechanges").style.display="block";
}
$('#details').submit(function(e){
    e.preventDefault();
    $.ajax({
        url:'/profilechanges',
